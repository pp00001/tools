--
--  The general stucture of the object is the following:
--
--
--   pmRxPwr<AlarmName>SeverityCode  OBJECT-TYPE
--     SYNTAX    INTEGER {
--                            disable                    (1),
--                            status-trapEnable          (2),
--                            status-trapEnableWithACK   (34),
--                            warning-trapEnable         (3),
--                            warning-trapEnableWithACK  (35),
--                            minor-trapEnable           (4),
--                            minor-trapEnableWithACK    (36),
--                            major-trapEnable           (5),
