        ENTERPRISE pdh-al 
        DESCRIPTION
            "This event is generated by  NE when a portServiceLosAlarm is cleared
             The data passed with the event are:
                1) portServiceId
                2) UnitId
                3) portServiceLabel
                4) portServiceInternalLosAlarm
                5) equipTrapAcknowledge code number"
        ::= 1404

----------------------------------------------------------------------------------
--  4.40 COMMAND LOG GROUP
--  This group is used to manage the command log.
----------------------------------------------------------------------------------

   commandLogTable        OBJECT-TYPE
        SYNTAX            SEQUENCE OF CommandRecord
        ACCESS            not-accessible
        STATUS            mandatory
        DESCRIPTION
            "Table with command  records"
        ::= {commandLog 1}
        
        
   commandLogRecord       OBJECT-TYPE
        SYNTAX            CommandRecord
        ACCESS            not-accessible
        STATUS            mandatory
        DESCRIPTION
            "Command record"
        INDEX { commandLogRecordId }
        ::= {commandLogTable 1}
        
        
   CommandRecord ::=
        SEQUENCE {
            commandLogRecordId
                 INTEGER,
            commandLogRecordRequestId
                 INTEGER,
            commandLogRecordObjectId
                 OBJECT IDENTIFIER,
            commandLogRecordObjectValue
                 DisplayString,
            commandLogRecordUserName
                 DisplayString,
            commandLogRecordSourceAddress
                 DisplayString,
            commandLogRecordTimeStamp
                 TimeTicks,
            commandLogRecordError
                 INTEGER
            }
            
            
   commandLogRecordId     OBJECT-TYPE
        SYNTAX            INTEGER
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "index of command log record"
        ::= {commandLogRecord 1}
        
        
   commandLogRecordRequestId    OBJECT-TYPE
        SYNTAX            INTEGER
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "request identifier of the packet"
        ::= {commandLogRecord 2}
        
        
   commandLogRecordObjectId     OBJECT-TYPE
        SYNTAX            OBJECT IDENTIFIER
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "identifier of the managed object"
        ::= {commandLogRecord 3}
        
        
   commandLogRecordObjectValue     OBJECT-TYPE
        SYNTAX            DisplayString     (SIZE(0..63))
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "value of the managed object displayed as a string"
        ::= {commandLogRecord 4}
        
        
   commandLogRecordUserName        OBJECT-TYPE
        SYNTAX            DisplayString     (SIZE(0..8))
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "Name of the user that sent the request"
        ::= {commandLogRecord 5}
        
        
   commandLogRecordSourceAddress     OBJECT-TYPE
        SYNTAX            DisplayString     (SIZE(0..16))
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "IP address of the remote machine that sent the request"
        ::= {commandLogRecord 6}
        
        
   commandLogRecordTimeStamp     OBJECT-TYPE
        SYNTAX            TimeTicks
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "The time as seconds when command is registered since 01/01/1970"
        ::= {commandLogRecord 7}
        
        
   commandLogRecordError     OBJECT-TYPE
        SYNTAX            INTEGER
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION 
            "Value returned by agent SNMP for the managed object"
        ::= {commandLogRecord 8}
        
        

----------------------------------------------------------------------------------
-- The following objects are used  to map the "action"
-- (delete record , transfer the log using NFS ...):
----------------------------------------------------------------------------------


   commandLogActionRequest     OBJECT-TYPE
        SYNTAX            INTEGER{
                                 delete (1),
                                 read (2)
                                 }
        ACCESS            write-only
        STATUS            mandatory
        DESCRIPTION
            "This leaf is used to delete record or to read it
            by FTP"
        ::= {commandLog 2}
        
        
   commandLogFtpFile     OBJECT-TYPE
        SYNTAX            DisplayString   (SIZE(0..255))
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "This leaf is reserved to file path where to
            transfer data"
        ::= {commandLog 3}
        
        
----------------------------------------------------------------------------------
-- The following objects are used  to enable/disable
-- writing of commands in the record
----------------------------------------------------------------------------------
        
        
   commandLogStartHourEnable     OBJECT-TYPE
        SYNTAX            INTEGER     (0..23)
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "Start hour of filter to enable/disable
            writing in the logger"
        DEFVAL            {0}
        ::= {commandLog 4}
        
        
   commandLogEndHourEnable     OBJECT-TYPE
        SYNTAX            INTEGER     (0..23)
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "End hour of filter to enable/disable
            writing in the logger"
        DEFVAL            {23}
        ::= {commandLog 5}
        
        
----------------------------------------------------------------------------------
-- The following objects are used  to filter
-- commands when an action (read or delete) is requested
----------------------------------------------------------------------------------
        
        
   commandLogStartTimeFilter     OBJECT-TYPE
        SYNTAX            TimeTicks
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "The events with EventTime greater than this object are read/delete from the log.
             Null value means no filter"
        DEFVAL            {0}
        ::= {commandLog 6}
        
        
   commandLogEndTimeFilter     OBJECT-TYPE
        SYNTAX            TimeTicks
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "The events with EventTime less than this object are read/delete from the log. 
             Null value means no filter"
        DEFVAL            {0}
        ::= {commandLog 7}
        
        
   commandLogObjectFilter     OBJECT-TYPE
        SYNTAX            OBJECT IDENTIFIER
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "Object identifier of filter to manage
            commands in the record. Pdh-Al value means no filter"
        DEFVAL            {pdh-al}
        ::= {commandLog 8}
        
        
   commandLogUserNameFilter     OBJECT-TYPE
        SYNTAX            DisplayString     (SIZE(0..8))
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "Name of the user that sent the request for the filter to manage
            commands in the record. ** value means no filter" 
        DEFVAL            {""}
        ::= {commandLog 9}
        
   
   commandLogSourceAddressFilter     OBJECT-TYPE
        SYNTAX            DisplayString     (SIZE(0..16))
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "IP address of source machine of filter to manage
            commands in the record. 0 value means no filter"
        DEFVAL            {"0"}
        ::= {commandLog 10}
        
   
   commandLogErrorFilter     OBJECT-TYPE
        SYNTAX            INTEGER
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "Error field for filter to manage commands in the record"
        DEFVAL            {-1}
        ::= {commandLog 11}
        
        
        
----------------------------------------------------------------------------------
-- The following objects are used  to manage
-- action in the FTP transfer
----------------------------------------------------------------------------------
      
     
   commandLogFtpStatus     OBJECT-TYPE
        SYNTAX            INTEGER{
                                 transferring (1),
                                 completed (2),
                                 interrupted (3),
                                 empty (4)
                                 }
        ACCESS            read-only
        STATUS            mandatory
        DESCRIPTION
            "This leaf is used for the status of transfer"
        DEFVAL            {completed}
        ::= {commandLog 12}
        
        
   commandLogFtpStatusTrapNotification     OBJECT-TYPE
        SYNTAX            INTEGER{
                                 trapDisable (1),
                                 trapEnable (2),
                                 trapEnableWithACK (3)
                                 }
        ACCESS            read-write
        STATUS            mandatory
        DESCRIPTION
            "Enable/disable the trap generation on FTP tranfer operation"
        DEFVAL            {trapEnable}
        ::= {commandLog 13}   
        
        
----------------------------------------------------------------------------------
-- The command logger group generates this trap
----------------------------------------------------------------------------------

    commandLogFtpStatusTrap  TRAP-TYPE
        ENTERPRISE pdh-al
        DESCRIPTION
            "This event is generated by SDH-NE when the status of FTP transfer is changed
             The data passed with the event are
             1) commandLogFtpStatus
             2) accessControlLoginIpAddress
             3) equipTrapAcknowledge code number"
        ::= 3001



END



