     ::= {zteuHisPMEntry 8}	 
	 
hisPMDescription OBJECT-TYPE
	 SYNTAX DisplayString
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "
     This variable presents description information for PMParameter
     "
     ::= {zteuHisPMEntry 9}	 
	 
hisPMAdditionalInfo     OBJECT-TYPE
     SYNTAX ObjectNVSList-T
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "
     This variable presents additional information
     "
     ::= {zteuHisPMEntry 10}	 

    
	zteuHisPMEntryGroup OBJECT-GROUP
	OBJECTS
   {
    	hisPMSource,
    	hisPMLayerRate,				
    	hisPMLocation,
		hisPMGranularity,		
		hisPMendOrElapsedTime,  
		hisPMParameter, 		
	    hisPMValue,   
	    hisPMUnit,
		hisPMDescription,
        hisPMAdditionalInfo
 	}
 	STATUS current
 	DESCRIPTION 
 	"An object group of historical performance  entry"       
	::= {zteuPfmObjects 21}
	
    END
