-- ********************************************************************
-- Trap Object Type Definition
-- ********************************************************************
ObjectType-TC ::=  TEXTUAL-CONVENTION
STATUS current
DESCRIPTION "
This textual convention presents unit of object value
'EMS'
'ManagedElement'
'TopologicalLink'
'PTP'
'CTP'
'EquipmentHolder'
'Equipment'
'CrossConnection'
'SNC'
"
SYNTAX OCTET STRING(SIZE (32))    
-- ********************************************************************

zteuSyncTrapAndInfo      OBJECT IDENTIFIER ::= { zteuTrap 1 }
zteuSystemTrapAndInfo    OBJECT IDENTIFIER ::= { zteuTrap 2 }
zteuAlarmTrapAndInfo     OBJECT IDENTIFIER ::= { zteuTrap 3 }
zteuMECommTrapAndInfo       OBJECT IDENTIFIER ::= { zteuTrap 6 }
zteuHeartBeatTrapAndInfo       OBJECT IDENTIFIER ::= { zteuTrap 7 }

zteuAlarmSyncInfo     OBJECT IDENTIFIER ::= { zteuAlarmTrapAndInfo 6 }
zteuAlarmInfo     OBJECT IDENTIFIER ::= { zteuAlarmTrapAndInfo 8 }
zteuSyncInfo      OBJECT IDENTIFIER ::= { zteuSyncTrapAndInfo 5 }
zteuSystemInfo    OBJECT IDENTIFIER ::= { zteuSystemTrapAndInfo 2 }
zteuMECommInfo       OBJECT IDENTIFIER ::= { zteuMECommTrapAndInfo 2 }
zteuHeartBeartInfo       OBJECT IDENTIFIER ::= { zteuHeartBeatTrapAndInfo 2 }
-- ********************************************************************
                   

-- ********************************************************************
-- TEXTUAL-CONVENTION Definition
-- ********************************************************************
   SystemNotifyType-TC ::= TEXTUAL-CONVENTION
   STATUS current
   DESCRIPTION " 
    This textual convention presents system notification
   "
   SYNTAX  OCTET STRING                   

   AlarmSyncTrapType-TC ::= TEXTUAL-CONVENTION
	   STATUS current
	   DESCRIPTION "
		   This textual convention presents AlarmSyncTrap Type
		   ast-SYNCSTART(1)        Start sync state
		   ast-SYNCCOMPLETE(2)      complete
		   ast-SYNCTERMINATE(3)		terminate by user
		   ast-UNKNOWN(4)        unknown
		  " 
	   SYNTAX INTEGER 
	   {
		  ast-SYNCSTART(1),
		  ast-SYNCCOMPLETE(2),
		  ast-SYNCTERMINATE(3),
		  ast-UNKNOWN(4)
	   }

