    ::= { zteuSyncInfo 4 }

creationObjectInfo  OBJECT-TYPE
        SYNTAX          ObjectNVSList-T 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                " This attribute presents object information,it is consist of
				a list of parameters,represented in name-value pairs format:
				<'',''>[,<'',''>,......]for ObjectNVSList-T,NamingAttributes-T,
				LayeredParameterList-T type parameters,the value should be 
				wrapped with '[]' if it isn't null,e.g. <'emsAdditionalInfo',
				'[<'egParaName','for example'>]'>
				Object: EMS			
						Name                      Value
						'emsUserLabel'            DisplayString, 
                        'emsType'                 DisplayString,      
                        'emsVersion'              DisplayString,
                        'emsAddress'              DisplayString,  
				        'emsCommState'			  Refer to 'CommunicationState-TC',represented in string format.
                        'emsAdditionalInfo'   	  Refer to 'ObjectNVSList-T'
                Object: ManagedElement
						'meUserLabel'                     DisplayString, 
						'meOwner'							DisplayString,
						'meLocation'                      DisplayString,
						'meVersion'                       DisplayString,
						'meProductName'					DisplayString,
						'meCommunicationState'			Refer to 'CommunicationState-TC,represented in string format.
						'meInSyncState'					Refer to 'BOOL-TC',represented in string format.           
						'meAddress'                       DisplayString,              
						'ifGateMe'                      Refer to 'BOOL-TC',represented in string format.
						'meCreatedTime'					Refer to 'Time-TC',represented in string format.            
						'meAdditionalInfo'				Refer to 'ObjectNVSList-T'
                Object: Equipment
						'eqUserLabel'             DisplayString,          
						'eqOwner'                 DisplayString,              
						'eqServiceState'          Refer to 'ServiceState-TC',represented in string format.
						'eqExpectedEquipmentObjectType'   Refer to 'EquipmentObjectType-TC',represented in string format.
						'eqInstalledEquipmentObjectType'  Refer to 'EquipmentObjectType-TC',represented in string format.
						'eqInstalledVersion'    DisplayString,
						'eqAdminStatus'         'INTEGER',represented in string format.
						'boardParameters'       Refer to 'ObjectNVSList-T',
						'eqAdditionalInfo'      Refer to 'ObjectNVSList-T'  
				Object: EquipmentHolder		
						'ehUserLabel'                    DisplayString,          
						'ehOwner'                        DisplayString,      
						'ehHolderType'                   Refer to 'EquipmentHolderType-TC',represented in string format. 
						'ehVersion'					     DisplayString,
						'ehExpectedOrInstalledEquipment' Refer to 'NamingAttributes-T',
						'ehAcceptableEquipmentTypeList'  Refer to 'EquipmentObjectTypeList-T',represented in string format.   
						'holderState'                    Refer to 'HolderState-TC',represented in string format.
						'ehAdditionalInfo'               Refer to 'ObjectNVSList-T'  
                Object: TerminationPoint
				   	    'tpUserLabel'           DisplayString,
						'tpType'                Refer to 'TPType-TC',represented in string format.        
						'tpConnectionState'     Refer to 'TPConnectionState-TC',represented in string format.
						'tpMappingMode'         Refer to 'TerminationMode-TC',represented in string format. 
						'tpDirection'           Refer to 'Directionality-TC',represented in string format. 
						'tpTransmissionParams'  Refer to 'LayeredParameterList-T',represented in string format.
						'tpAdminStatus'         INTEGER,represented in string format.
						'tpOperStatus'          INTEGER,represented in string format.
						'tpAdditionalInfo'      Refer to 'ObjectNVSList-T' 	
                Object: TopologicalLink
						'tlUserLabel'            DisplayString, 
						'tlDirection'            INTEGER,represented in string format.      
						'tlLayerRate'            Refer to 'LayerRate-TC',represented in string format.
						'tlAEnd'				   Refer to 'NamingAttributes-T',
						'tlZEnd'				   Refer to 'NamingAttributes-T',
						'tlAdditionalInfo'   	  Refer to ' ObjectNVSList-T'.      
				"
        ::= { zteuSyncInfo 5 }

creationAdditionalInfo OBJECT-TYPE
        SYNTAX          ObjectNVSList-T 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                " This attribute presents additional information
                "
        ::= { zteuSyncInfo 6 } 

    
