UnitransMib-MS-trap  
DEFINITIONS ::= BEGIN
IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
    enterprises, Integer32, Unsigned32, IpAddress
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION, DisplayString, TimeStamp, DateAndTime
        FROM SNMPv2-TC  
    MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
        FROM SNMPv2-CONF
        zteUnitransMS
        FROM 
        UnitransMib-MS-root
        BOOL-TC,Time-TC,Unit-TC,   
        ObjectNVSList-T,NamingAttributes-T,LayerRate-TC,LayerRate-TC
        FROM 
        UnitransMib-MS-common          
        
        PMParameterName-TC,PMLocation-TC,PMValue-TC,Granularity-TC,PMThresholdType-TC,PMValue-TC
        FROM 
        UnitransMib-MS-performance    
