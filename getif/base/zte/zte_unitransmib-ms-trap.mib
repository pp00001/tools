        SYNTAX          OCTET STRING
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This attribute presents a trap identifier"
        ::= { zteuSyncInfo  1 }

creationObjectName OBJECT-TYPE
        SYNTAX          NamingAttributes-T 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This attribute presents object name,
                For details,please refer NamingAttributes-T's definition"
