                        singleModeS1(4),
                        singleModeS2(5),
                        singleModeS3(6)
                      } 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Fiber type of the port."
        ::= { portEntry 3 }

portConnectorType OBJECT-TYPE
