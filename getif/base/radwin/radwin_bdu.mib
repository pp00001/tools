-- the bduVLAN group
                                       
-- Port level table ----------------------------------------
portVLANTable OBJECT-TYPE 
        SYNTAX        SEQUENCE OF PortVLANEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of 802.1Q VLAN port entries."
        ::= { bduVLAN 2 }

portVLANEntry OBJECT-TYPE
        SYNTAX        PortVLANEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry containing information about one port 802.1Q VLAN entry."
        INDEX         { portIndex }
        ::= { portVLANTable 1 }

PortVLANEntry ::=
        SEQUENCE {
                pIndex              INTEGER,
               	defaultTag      	INTEGER,
                ingTagRemove       	INTEGER,
                egTagInsert        	INTEGER,    
                en8021q	        	INTEGER,
                egTagRemove       	INTEGER,                    
                egDoubleTagEn      	INTEGER,                    
                ingDoubleTagEn     	INTEGER,
                pbVlanMembership   	INTEGER
               	}

pIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..32)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies this port
					  within the module."
        ::= { portVLANEntry 1 }

defaultTag OBJECT-TYPE
        SYNTAX        INTEGER (0..4095)
