portTable OBJECT-TYPE 
        SYNTAX SEQUENCE OF PortEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ethernet port entries."
        ::= { bduPorts 1 }

portEntry OBJECT-TYPE
        SYNTAX        PortEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry containing information about one ethernet port."
        INDEX         { portIndex }
        ::= { portTable 1 }

PortEntry ::=
        SEQUENCE {
                portIndex            INTEGER,
				portPhyType          INTEGER,   
                portFiberType        INTEGER,
                portConnectorType    INTEGER,        
                portWavelength       INTEGER,                        
                portDescr            DisplayString,  
                portName             DisplayString,
                portAdminMode        INTEGER,
               	portOperMode         INTEGER,
                portAdminSpeed       INTEGER,
                portOperSpeed        INTEGER,
                portAdminDuplex      INTEGER,
                portOperDuplex       INTEGER,
                portAdminStatus      INTEGER,        
                portOperStatus       INTEGER,
               	portAdminFEF         INTEGER,
               	portOperFEF          INTEGER,		
               	portCntReset         INTEGER,		
               	portTxCnt            Counter32,
                portLinkStatus       INTEGER,
                portActivityStatus   INTEGER,
                portCollision        INTEGER,                      			        
                portAdminMD          INTEGER,
                portOperMD           INTEGER,
               	portTxBroadcastCnt   Counter32,
               	portTxMulticastCnt   Counter32,
               	portAdminFC		 	 INTEGER,
               	portOperFC		 	 INTEGER,
               	portMonitorMode	 	 INTEGER,
               	portMonitorDest	 	 INTEGER,  
               	portMcastEnable      INTEGER,
               	portComboPrefMedia   INTEGER,
                portComboLinkMedia   INTEGER,               	
               	portPhysCompliance 	 INTEGER,
               	portAppCompliance 	 INTEGER
        }

portIndex OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies this port."
        ::= { portEntry 1 }

portPhyType OBJECT-TYPE
        SYNTAX        INTEGER {
                      	other(1),       		-- none of the following
                        copper(2),
                        normalFormFiber(3),
                        smallFormFiber(4)
                      } 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Phy type of the port."
        ::= { portEntry 2 }

portFiberType OBJECT-TYPE
        SYNTAX        INTEGER {
                      	other(1),       		-- none of the following
                        multiMode(2),
