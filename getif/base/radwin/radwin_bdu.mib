	::= { bduVLAN 7 }
	
chkpntVtbl OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (64))
    MAX-ACCESS read-only
	STATUS current
	DESCRIPTION 	 "Checkpoint for module level parameters."
	::= { bduVLAN 8 }

transVIDen OBJECT-TYPE
    SYNTAX INTEGER {
      na(0),
      enable(1),
      disable(2)
    } 
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Enable/Disable transparent VID functionality."
	::= { bduVLAN 10 }       
                                       
transVIDtag OBJECT-TYPE
        SYNTAX        INTEGER (0..4095)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "VLAN tag transparent VID."
        ::= { bduVLAN 11 }

transVIDtrunkPorts OBJECT-TYPE
        SYNTAX        INTEGER (0..65535)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "Trunk port membership settings in binary format. bit 0 (LSB) represents port 1
        			  bit 7 - represents port 8 ...
        			  i.e. 0x0000 means no ports are trunk ports. and 0x0011 means that
        			  ports 1 and 5 are trunk ports."
        ::= { bduVLAN 12 }
                                       
