-- 802.1Q table ----------------------------------------
vlan8021qConfigurationTable OBJECT-TYPE 
        SYNTAX        SEQUENCE OF Vlan8021qConfigurationEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of port 802.1Q VLAN configuration entries."
        ::= { bduVLAN 4 }

vlan8021qConfigurationEntry OBJECT-TYPE
        SYNTAX        Vlan8021qConfigurationEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry containing information about one 802.1Q VLAN configuration 
        			  entry."
        INDEX         { vlan8021qIx }
        ::= { vlan8021qConfigurationTable 1 }

Vlan8021qConfigurationEntry ::=
        SEQUENCE {
                vlan8021qIx                 INTEGER,
				vlan8021qTag	            INTEGER,                        
               	vlan8021qPortMembership 	INTEGER        
        }

vlan8021qIx OBJECT-TYPE
        SYNTAX        INTEGER (1..16)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies this vlan entry within a Module/Stack."
        ::= { vlan8021qConfigurationEntry 1 }

vlan8021qTag OBJECT-TYPE
        SYNTAX        INTEGER (0..4095)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "VLAN tag ID of a specific entry."
        ::= { vlan8021qConfigurationEntry 2 }

vlan8021qPortMembership OBJECT-TYPE
        SYNTAX        INTEGER (0..65535)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Port membership settings in binary format. bit 0 (LSB) represents port 1
        			  bit 7 - represents port 8 ...
        			  i.e. 0x0000 means no ports are members in this VLAN. and 0x0011 means that
        			  ports 1 and 5 are members in this VLAN."
        ::= { vlan8021qConfigurationEntry 3 }
 
vlan8021qConfigurationCommands OBJECT IDENTIFIER    ::= { bduVLAN 5 }

vlan8021qCommandIx OBJECT-TYPE
	SYNTAX INTEGER (1..16)
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Used for temporary storage of the VLAN index to add, edit
	                 or remove from the 802.1Q VLAN configuration table."
	::= { vlan8021qConfigurationCommands 1 }

vlan8021qCommandTag OBJECT-TYPE
	SYNTAX INTEGER (0..4095)
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Used for temporary storage of the VLAN tag to add, edit
	                 or remove from the 802.1Q VLAN configuration table."
	::= { vlan8021qConfigurationCommands 2 }

vlan8021qCommandPortMembership OBJECT-TYPE
	SYNTAX INTEGER (0..65535)
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Used for temporary storage of the Port membership settings to add, edit
	                 or remove from the 802.1Q VLAN configuration table."
	::= { vlan8021qConfigurationCommands 3 }

vlan8021qCommandOpcode OBJECT-TYPE
	SYNTAX INTEGER {
	  addORedit(1),
	  remove(2)
	} 
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Activation command: setting this OID with value 1(add or edit) adds or updates
	                 the VLAN configuration entry to the VLAN configuration table. setting this OID 
	                 with value 2(remove) deletes the entry from the 802.1Q VLAN configuration table."
	::= { vlan8021qConfigurationCommands 4 }
                                       
globalVlanMode OBJECT-TYPE
    SYNTAX INTEGER {
      enable8021q(1),
      disable(2),   
      enablePortBased(3)
    } 
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION 	 "Select VLAN mode (802.1Q or port-based) on entire switch."
	::= { bduVLAN 6 }       
	
chkpntPtbl OBJECT-TYPE
