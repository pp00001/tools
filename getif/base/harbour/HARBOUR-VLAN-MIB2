HARBOUR-VLAN-MIB2 DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    IpAddress          FROM SNMPv2-SMI
    RowStatus          FROM SNMPv2-TC
    DisplayString      FROM RFC1213-MIB
    harbourAgent       FROM HARBOUR-BASE-MIB
    Unsigned32         FROM SNMPv2-SMI
    PortList           FROM Q-BRIDGE-MIB
    VlanIndex,
    VlanId             FROM Q-BRIDGE-MIB
    harbourSystem      FROM HARBOUR-SYSTEM-MIB;


harbourVlan  MODULE-IDENTITY
    LAST-UPDATED  "200311110000Z"
    ORGANIZATION  "harbourNetworks, Inc."
    CONTACT-INFO  "www.harbournetworks.com"
    DESCRIPTION
            "The MIB module defining objects for VLAN ."
    ::= { harbourAgent 10 }


-- The objects are arranged into the following groups:

    harbourVlanBase         OBJECT IDENTIFIER ::= { harbourVlan 1 }

    harbourVlanGroup        OBJECT IDENTIFIER ::= { harbourVlan 2 }

    harbourSuperVlanGroup   OBJECT IDENTIFIER ::= { harbourVlan 3 }


-- harbourVlanBase group is copied from dot1qBase of Q-BRIDGE-MIB

harbourVlanVerNumber OBJECT-TYPE
    SYNTAX      INTEGER { version1(1) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The version number of IEEE 802.1Q that this device
            supports."
    REFERENCE
            "IEEE 802.1Q/D11 Section 12.10.1.1"
    ::= { harbourVlanBase 1 }

harbourVlanMaxVid OBJECT-TYPE
    SYNTAX      VlanId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The maximum IEEE 802.1Q VLAN ID that this device
            supports."
    REFERENCE
            "IEEE 802.1Q/D11 Section 9.3.2.3"
    ::= { harbourVlanBase 2 }

harbourVlanMaxSupportedVlans OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The maximum number of IEEE 802.1Q VLANs that this
            device supports."
    REFERENCE
            "IEEE 802.1Q/D11 Section 12.10.1.1"
    ::= { harbourVlanBase 3 }

harbourVlanNumVlans OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The current number of IEEE 802.1Q VLANs that are
         configured in this device."
    REFERENCE
        "IEEE 802.1Q/D11 Section 12.7.1.1"
    ::= { harbourVlanBase 4 }

harbourVlanGvrpStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    enabled (1),
                    disabled (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The administrative status requested by management for
            GVRP.  The value enabled(1) indicates that GVRP should
            be enabled on this device, on all ports for which it has
            not been specifically disabled.  When disabled(2), GVRP
            is disabled on all ports and all GVRP packets will be
            forwarded transparently.  This object affects all GVRP
            Applicant and Registrar state machines.  A transition
            from disabled(2) to enabled(1) will cause a reset of all
            GVRP state machines on all ports."
    DEFVAL     { enabled }
    ::= { harbourVlanBase 5 }

harbourNewVlanIndex OBJECT-TYPE
    SYNTAX      VlanIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A harbourVlanIndex of last created vlan.
            This object just promote NMS programmer's convenience."
    ::= { harbourVlanBase 6 }

harbourVlanMode OBJECT-TYPE
    SYNTAX      INTEGER {
                       multiple(1),
                       single(2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object indicate the vlan mode."
    DEFVAL     { multiple }
    ::= { harbourVlanBase 7 }

   harbourBaseLevel OBJECT-TYPE
    SYNTAX      INTEGER(0..15)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object indicate the vlan baselevel."
    DEFVAL     { 7 }
    ::= { harbourVlanBase 8 }

-- Harbour Vlan Group

-- Harbour Vlan Prop Table

harbourVlanPropTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourVlanPropEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "harbour vlan's properties."
    ::= { harbourVlanGroup 1 }

harbourVlanPropEntry OBJECT-TYPE
    SYNTAX      HarbourVlanPropEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbourVlanPropTable."
    INDEX       { harbourVlanIndex }
    ::= { harbourVlanPropTable 1 }

HarbourVlanPropEntry ::= SEQUENCE {
    harbourVlanIndex                        VlanIndex,
    harbourVlanName                         DisplayString,
    harbourVlanAllPorts                     PortList,
    harbourVlanForbiddenPorts               PortList,
    harbourVlanUntaggedPorts                PortList,
    harbourVlanID                           VlanId,
    harbourVlanRowStatus                    RowStatus,
    harbourVlanType                         INTEGER,
    harbourVlanifIndex                      INTEGER,
    harbourVlanIP                           IpAddress,
    harbourVlanMask                         IpAddress,
    harbourVlanConsole                      INTEGER,
    harbourVlanInputPorts                   PortList,
    harbourVlanIGMPPorts                    PortList,
    harbourVlanUserLimit                    INTEGER,
    harbourVlanPPPOE                        INTEGER,
    harbourVlanUserLimitstatus              INTEGER,
    harbourVlanPPPOEListen                  INTEGER,
    pppLcpFastStart                         INTEGER,
    pppLcpDelayTime                         INTEGER,
    pppIpcpAcceptAddress                    INTEGER,
    pppIpcpDnsAcceptPrimaryIpAddress        DisplayString,
    pppIpcpDnsAcceptSecondaryIpAddress      DisplayString,
    pppIpcpDnsAcceptAddress                 INTEGER,
    pppIpcpWinsAcceptPrimaryIpAddress       DisplayString,
    pppIpcpWinsAcceptSecondaryIpAddress     DisplayString,
    pppIpcpWinsAcceptAddress                INTEGER,
    pppAuthenticationStatus                 INTEGER,
    pppAuthenticationProtocol               INTEGER,
    pppTimeoutRetryNum                      INTEGER,     --Number
    pppTimeoutAuthenticatonTime             INTEGER,
    pppPeerIpAddressPoolConf                DisplayString,
    pppPeerIpAddressDhcpProxy               INTEGER,
    pppPeerIpAddressPoolName                DisplayString,
    pppLcpInterval                          INTEGER,
    pppLcpRetransmit                        INTEGER
}

harbourVlanIndex OBJECT-TYPE
    SYNTAX      VlanIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A subscript of vlan's array which is used by 2 layer.
            This object is the same as dot1qVlanIndex of Q-BRIDGE-MIB."
    ::= { harbourVlanPropEntry 1 }

harbourVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object is the same as dot1qVlanStaticName of Q-BRIDGE-MIB."
    ::= { harbourVlanPropEntry 2 }

harbourVlanAllPorts OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object is the same as dot1qVlanStaticEgressPorts of
            Q-BRIDGE-MIB."
    ::= { harbourVlanPropEntry 3 }

harbourVlanForbiddenPorts OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object is the same as dot1qVlanForbiddenEgressPorts
            of Q-BRIDGE-MIB."
    ::= { harbourVlanPropEntry 4 }

harbourVlanUntaggedPorts OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object is the same as dot1qVlanStaticUntaggedPorts of
            Q-BRIDGE-MIB."
    ::= { harbourVlanPropEntry 5 }

harbourVlanID OBJECT-TYPE
    SYNTAX      VlanId
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "Vlan's vid."
    ::= { harbourVlanPropEntry 6 }

harbourVlanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the status of this entry."
    ::= { harbourVlanPropEntry 7 }

harbourVlanType OBJECT-TYPE
    SYNTAX      INTEGER {
                    normalvlan (0),
                    supervlan (1),
                    subvlan (2)
                }
        MAX-ACCESS      read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the vlan's type."
    ::= { harbourVlanPropEntry 8 }

harbourVlanifIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "This object indicates vlan's ifIndex."
    ::= { harbourVlanPropEntry 9 }

harbourVlanIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates vlan's IP address."
    ::= { harbourVlanPropEntry 10 }

harbourVlanMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
