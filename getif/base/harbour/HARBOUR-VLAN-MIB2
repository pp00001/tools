    ::= { harbourVlanPropEntry 13 }

harbourVlanIGMPPorts OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates vlan's IGMP ports."
    ::= { harbourVlanPropEntry 14 }

harbourVlanUserLimit OBJECT-TYPE
    SYNTAX      INTEGER(0.. 1024)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The max number of user can connect to this vlan."
    ::= { harbourVlanPropEntry 15 }

harbourVlanPPPOE OBJECT-TYPE
    SYNTAX      INTEGER {
                    on (1),
                    off (2),
                    invalid (3)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates vlan's PPPOE status."
    ::= { harbourVlanPropEntry 16 }

harbourVlanUserLimitstatus OBJECT-TYPE
    SYNTAX      INTEGER{
                    nouserlimit(0),
                    userlimit(1)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The max number of user can connect to this vlan."
    ::= { harbourVlanPropEntry 17 }

harbourVlanPPPOEListen OBJECT-TYPE
    SYNTAX      INTEGER{
                    yes(0),
                    no(1)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The object use for config dhcpc-proxy add vlan to listen."
    ::= { harbourVlanPropEntry 18 }

pppLcpFastStart OBJECT-TYPE
    SYNTAX      INTEGER {
                    fast-start (1),
                    nofast-start (2)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "Config interface virtual-template ppp
            lcp fast-start or no fast-start."
    ::= { harbourVlanPropEntry 19 }

pppLcpDelayTime OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "Config interface virtual-template ppp
            lcp delay time,if the value equal to 0
            It's no ppp lcp delay."
    ::= { harbourVlanPropEntry 20 }

pppIpcpAcceptAddress OBJECT-TYPE
    SYNTAX      INTEGER{
                    none (0),
                    accept (1)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
