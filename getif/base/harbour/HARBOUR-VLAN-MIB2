    ::= { harbourVlanPropEntry 36 }


-- Harbour SuperVlan Group

harbourVlanSearchListAgingTime OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is to set the searchlist aging time
            for all of the super vlan."
    ::= { harbourSuperVlanGroup 1 }

-- Harbour SuperVlan Prop Table

harbourSuperVlanPropTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSuperVlanPropEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "harbour SuperVlan's properties."
    ::= { harbourSuperVlanGroup 2 }

harbourSuperVlanPropEntry OBJECT-TYPE
    SYNTAX      HarbourSuperVlanPropEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbourSuperVlanPropTable."
    INDEX      { harbourSuperVlanIndex }
    ::= { harbourSuperVlanPropTable 1 }

HarbourSuperVlanPropEntry ::= SEQUENCE {
    harbourSuperVlanIndex      VlanIndex,
    harbourSuperVlanProxyARP   INTEGER,
    HarbourSuperVlanName       DisplayString
}

harbourSuperVlanIndex OBJECT-TYPE
    SYNTAX      VlanIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A SuperVlan's harbourVlanIndex."
    ::= { harbourSuperVlanPropEntry 1 }

harbourSuperVlanProxyARP OBJECT-TYPE
    SYNTAX      INTEGER {
                   disable (0),
                   enable (1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Enable or disable a SuperVlan's proxy ARP function."
    ::= { harbourSuperVlanPropEntry 2 }

HarbourSuperVlanName OBJECT-TYPE
    SYNTAX     DisplayString (SIZE(0 .. 32))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
            "name of super vlan."
    ::= { harbourSuperVlanPropEntry 3 }

-- Harbour SuperVlan Table

harbourSuperVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSuperVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "This table describe relationship between SuperVlans
            and SubVlans."
::= { harbourSuperVlanGroup 3 }

harbourSuperVlanEntry OBJECT-TYPE
    SYNTAX      HarbourSuperVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbourSuperVlanTable."
    INDEX           { harbourSuperVlanIndex, harbourSubVlanIndex }
::= { harbourSuperVlanTable 1 }

HarbourSuperVlanEntry ::= SEQUENCE {
    harbourSubVlanIndex        VlanIndex,
    harbourSuperVlanName       DisplayString,
    harbourSubVlanName         DisplayString,
    harbourSuperVlanRowStatus  RowStatus
}

harbourSubVlanIndex OBJECT-TYPE
    SYNTAX      VlanIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A SubVlan's harbourVlanIndex."
    ::= { harbourSuperVlanEntry 1 }

harbourSuperVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "A SuperVlan's harbourVlanName."
    ::= { harbourSuperVlanEntry 2 }

harbourSubVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A SubVlan's harbourVlanName."
    ::= { harbourSuperVlanEntry 3 }

harbourSuperVlanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the status of this entry."
    ::= { harbourSuperVlanEntry 4 }

-- Harbour SuperVlan ProxyARP Filter Table

harbourSuperVlanProxyARPFilterTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSuperVlanProxyARPFilterEntry
    MAX-ACCESS  not-accessible
    STATUS      obsolete
    DESCRIPTION
            "This table describe proxy ARP filter in a SuperVlan."
    ::= { harbourSuperVlanGroup 4 }

harbourSuperVlanProxyARPFilterEntry OBJECT-TYPE
    SYNTAX      HarbourSuperVlanProxyARPFilterEntry
    MAX-ACCESS  not-accessible
    STATUS      obsolete
    DESCRIPTION
            "An entry in the harbourSuperVlanProxyARPFilterTable."
    INDEX       { harbourSuperVlanIndex, proxyARPFilterIndex }
::= { harbourSuperVlanProxyARPFilterTable 1 }

HarbourSuperVlanProxyARPFilterEntry ::= SEQUENCE {
    proxyARPFilterIndex      INTEGER,
    SuperVlanName            DisplayString,
    srcSubVlanName           DisplayString,
    destSubVlanName          DisplayString,
    proxyARPFilterRowStatus  RowStatus
}

proxyARPFilterIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      obsolete
    DESCRIPTION
            "A sequence number from 1."
    ::= { harbourSuperVlanProxyARPFilterEntry 1 }

SuperVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      obsolete
    DESCRIPTION
            "SuperVlan's name."
    ::= { harbourSuperVlanProxyARPFilterEntry 2 }

srcSubVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      obsolete
    DESCRIPTION
            "Source SubVlan's name."
    ::= { harbourSuperVlanProxyARPFilterEntry 3 }

destSubVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      obsolete
    DESCRIPTION
            "Destination SubVlan's name."
    ::= { harbourSuperVlanProxyARPFilterEntry 4 }

proxyARPFilterRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      obsolete
    DESCRIPTION
            "This object indicates the status of this entry."
    ::= { harbourSuperVlanProxyARPFilterEntry 5 }

-- Harbour SuperVlan ARP Group Table

harbourSuperVlanARPGroupTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSuperVlanARPGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "This table describe ARP Group in a SuperVlan."
    ::= { harbourSuperVlanGroup 5 }

harbourSuperVlanARPGroupEntry OBJECT-TYPE
    SYNTAX      HarbourSuperVlanARPGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbourSuperVlanARPGroupTable."
    INDEX       { harbourSuperVlanIndex, arpGroupIndex }
    ::= { harbourSuperVlanARPGroupTable 1 }

HarbourSuperVlanARPGroupEntry ::= SEQUENCE {
    arpGroupIndex            INTEGER,
    arpGroupName             DisplayString,
    arpGroupPortList         PortList,
    arpGroupRowStatus        RowStatus
}

arpGroupIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A sequence number from 1."
    ::= { harbourSuperVlanARPGroupEntry 1 }

arpGroupName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "arp Group's name."
    ::= { harbourSuperVlanARPGroupEntry 2 }

arpGroupPortList OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the ports in the arp group of the super vlan."
    ::= { harbourSuperVlanARPGroupEntry 3 }

arpGroupRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the status of this entry."
    ::= { harbourSuperVlanARPGroupEntry 4 }

-- Harbour SubVlan Table

harbourSubVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSubVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "This table describe ARP Group in a SuperVlan."
    ::= { harbourSuperVlanGroup 6 }

 harbourSubVlanEntry OBJECT-TYPE
    SYNTAX      HarbourSubVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbourSubVlanTable."
    INDEX       { SubVlanIndex }
    ::= { harbourSubVlanTable 1 }

HarbourSubVlanEntry ::= SEQUENCE {
    SubVlanIndex             INTEGER,
    SubVlanName              DisplayString,
    SubVlanPort              INTEGER,
    superVlanName            DisplayString,
    SubVlanType              INTEGER,
    SubVlanTagFlag           INTEGER,
    SubVlanRowStatus         RowStatus
}

SubVlanIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "This is the sub vlan's VID!"
    ::= { harbourSubVlanEntry 1 }

SubVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "SubVlan's name."
    ::= { harbourSubVlanEntry 2 }

SubVlanPort OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The port number of the sub vlan."
    ::= { harbourSubVlanEntry 3 }

superVlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0 .. 32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "the name of the Super vlan including this SubVlan."
    ::= { harbourSubVlanEntry 4 }

SubVlanType OBJECT-TYPE
    SYNTAX      INTEGER { changable(1),unchangable(2) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The type indicates whether this vlan can be configed or deleted."
    ::= { harbourSubVlanEntry 5 }

SubVlanTagFlag OBJECT-TYPE
    SYNTAX      INTEGER { tag(1),untag(2)}
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The type indicates whether this vlan is tagged or untaged."
    ::= { harbourSubVlanEntry 6 }

SubVlanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This object indicates the status of this entry."
    ::= { harbourSubVlanEntry 7 }


END