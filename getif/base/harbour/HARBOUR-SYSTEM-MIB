                    NH-PA-1GE(612),
                    NH-PA-POS(613),
                    NH-PA-8S(614),
                    NH-PA-8E1(615),
                    NH-PA-16E1(616),
                                    
                    NH-PA-8CE1(617),
                    NH-PA-16CE1(618),
                    NH-PA-4CE1(619),
                    NH-PA-CPOSOC3SML(620),
                    NH-PA-CPOSOC3SMI(621),
                    NH-PA-CPOSOC3MM(622),
                    NH-PA-CPOSSFP(623),
                    NH-PA-ATMOC3SML(624),
                    NH-PA-ATMOC3SMI(625),
                    NH-PA-ATMOC3MM(626),
                    NH-PA-ATMSFP(627),
                    NH-PA-CE3(628),
                    NH-PA-2FESFP(629),      
                                    
                    NH-PA-POS-OC3SML(701),
                    NH-PA-POS-OC3SMI(702),
                    NH-PA-POS-OC3MM(703) 
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The module type inserted into the slot. It is possible
            to have a slot inserted into the slot even though
            harbourSlotConfiguredType is none(1) in later
            release version."
    ::= { harbourSlotModuleEntry 4 }

harbourSlotModuleState OBJECT-TYPE
    SYNTAX      INTEGER {
                    notPresent(1),
                    testing(2),
                    mismatch(3),
                    failed(4),          -- not work
                    operational(5),     -- work well
                    removing(6),        -- remove key down
                    powerdown(7),       -- ready to plug out
                    unknow(8)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The state of the module inserted in this slot."
    ::= { harbourSlotModuleEntry 5 }

harbourSlotModuleFirstPortNumber OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The portIndex of the first port on the module."
    ::= { harbourSlotModuleEntry 6 }

harbourSlotModuleNumberOfPorts OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The number of ports on the  module."
    ::= { harbourSlotModuleEntry 7 }

harbourSlotModuleSerialNumber OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The serial number of the module inserted in this slot."
    ::= { harbourSlotModuleEntry 8 }

harbourDeviceVersion OBJECT-TYPE
    SYNTAX      INTEGER {
                    v1(1),
                    v2(2)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "To show the version of device if diffrent versions belong to
            the same type product."
    ::= { harbourChassisGroup 6  }


-- Standalone Group
-- Features of standalone products, such as Flex series and Micro series

harbourStandAloneGroup OBJECT IDENTIFIER ::=    { harbourSystem 3}

-- Standalone Slot Table

harbourSASlotTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourSASlotModuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A table containing information about each slot in the
            standalone product's slot."
    ::= { harbourStandAloneGroup 5 }

harbourSASlotModuleEntry OBJECT-TYPE
    SYNTAX      HarbourSASlotModuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A table entry containing information about the module in
            each slot of the stand alone product."
    INDEX       { harbourSASlotNumber }
    ::= { harbourSASlotTable 1 }

HarbourSASlotModuleEntry ::= SEQUENCE {
    harbourSASlotNumber                     INTEGER,
    harbourSASlotName                       DisplayString,
    harbourSASlotModuleConfiguredType       INTEGER,
    harbourSASlotModuleInsertedType         INTEGER,
    harbourSASlotModuleState                INTEGER,
    harbourSASlotModuleFirstPortNumber      INTEGER,
    harbourSASlotModuleNumberOfPorts        INTEGER,
    harbourSASlotModuleSerialNumber         DisplayString
}

harbourSASlotNumber OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A unique integer identifying the particular slot
            in the chassis."
    ::= { harbourSASlotModuleEntry 1 }

harbourSASlotName   OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (0..100))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "A name identifying the particular slot in the chassis,
            Descriptive, such as FirstSlot,SecondSlot and so on."
    ::= { harbourSASlotModuleEntry 2 }

