              ::= { adsl2PortDeltConfig 1 }  
              
          adsl2PortServiceMode  OBJECT-TYPE
              SYNTAX  INTEGER { gdmtbis(1)}
              ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "For Delt ,you should config port service mode is g.dmt.bis ."
              ::= { adsl2PortDeltConfig 2 }  
              
            adsl2PortDeltStart  OBJECT-TYPE
              SYNTAX  INTEGER {start(1),abort(2)}
              ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "If port is up ,you can config delt start."
              ::= { adsl2PortDeltConfig 3 } 
              
             adsl2PortDeltState  OBJECT-TYPE
              SYNTAX  INTEGER {idle(1),data(2),handshake(3),training(4),discovery(5),ll_test(6),
                                dl_test(7),training_delt(8),delt(9),unknown(10)}
              ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "When state is delt ,you can read datas."
              ::= { adsl2PortDeltConfig 4 } 
          	
          
        
          adsl2PortDeltStats    OBJECT IDENTIFIER ::= { adsl2PortDelt 2 }
                       
        
