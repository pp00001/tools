--StatisticGroup
	harbour802dot1xClearStatistic OBJECT-TYPE

		SYNTAX  INTEGER {
		  	ready(0),
		  	clearing(1)
		}

		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			" Read 0 means is ready to clear statistic.
			  Set to 1 means to clear all 802dot1x Statistic."

		::= {harbour802dot1xStatisticGroup 1}

	harbour802dot1xStatisticTable 	OBJECT-TYPE
		SYNTAX SEQUENCE OF harbour802dot1xStatisticEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"The table of dot1x statistics ."
		::= {harbour802dot1xStatisticGroup 2}


	harbour802dot1xStatisticEntry 	OBJECT-TYPE
		SYNTAX  	harbour802dot1xStatisticEntry
		MAX-ACCESS  	not-accessible
		STATUS 		mandatory
		DESCRIPTION
				"A table entry containing information about the module in
				each user of the server."
		INDEX	  	{
					harbour802dot1xStatisticItem
		}
		::= { harbour802dot1xStatisticTable 1 }

 	harbour802dot1xStatisticEntry ::=
	   	SEQUENCE {

			harbour802dot1xStatisticItem	    	INTEGER,
			harbour802dot1xStatisticPackets 	INTEGER
	 	}


	harbour802dot1xStatisticItem   	OBJECT-TYPE
		SYNTAX  	INTEGER{
			EAPOL-StartFromSupp(1),EAP-Req-IdFromNAS(2),EAP-Resp-IdFromSupp(3),
			Access-Req-IdFromNAS(4),Challenge(5),EAP-Req-MD5(6),EAP-Resp-MD5(7),
	   		Access-Req-MD5(8),Accept(9),EAP-Success(10),EAPOL-Logoff(11),Reject(12),
			EAP-Failure(13)
	 	}
		MAX-ACCESS  	read-only
		STATUS 		mandatory
		DESCRIPTION
				"Dot1x Statistic Item."
		::= { harbour802dot1xStatisticEntry 1 }

	harbour802dot1xStatisticPackets 	OBJECT-TYPE
		SYNTAX  			INTEGER
		ACCESS  			read-only
		STATUS 				mandatory
		DESCRIPTION
			"This object denotes packet count in Dot1x Statistic."
		::={ harbour802dot1xStatisticEntry 2 }



	harbour802dot1xAccountSatatisticTable 	OBJECT-TYPE
		SYNTAX SEQUENCE OF harbour802dot1xAccountSatatisticEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"The table of accounting statistics."
		::= {harbour802dot1xStatisticGroup 3}


	harbour802dot1xAccountSatatisticEntry 	OBJECT-TYPE
		SYNTAX  	harbour802dot1xAccountSatatisticEntry
		MAX-ACCESS  	not-accessible
		STATUS 		mandatory
		DESCRIPTION
				"A table entry containing information about the module in
				each user of the server."
		INDEX	  	{
					harbour802dot1xAccountSatatisticItem
		}
		::= { harbour802dot1xAccountSatatisticTable 1 }

 	harbour802dot1xAccountSatatisticEntry ::=
	   	SEQUENCE {
			harbour802dot1xAccountSatatisticItem	    INTEGER,
			harbour802dot1xAccountSatatisticPackets 	INTEGER
	 	}

	harbour802dot1xAccountSatatisticItem   	OBJECT-TYPE
		SYNTAX  	INTEGER {
			StartSendReq(1),StartGotResp(2),StopSentReq(3),
			StopGotResp(4),TotalSentReq(5),TotalGotResp(6)
	 	}
		MAX-ACCESS  	read-only
		STATUS 		mandatory
		DESCRIPTION
				"Account Satatistic Item."
		::= { harbour802dot1xAccountSatatisticEntry 1 }

	harbour802dot1xAccountSatatisticPackets	OBJECT-TYPE
		SYNTAX  			INTEGER
		ACCESS  			read-only
		STATUS 				mandatory
		DESCRIPTION
			"This object denotes packet count in Account Satatistic."
		::={ harbour802dot1xAccountSatatisticEntry 2 }

