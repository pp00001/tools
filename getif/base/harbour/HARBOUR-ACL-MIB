    aclRuleIndex            INTEGER,
    aclRuleDestIP           IpAddress,
    aclRuleDestIPMask       IpAddress,
    aclRuleDestPort         INTEGER,
    aclRuleSourceIP         IpAddress,
    aclRuleSourceIPMask     IpAddress,
    aclRuleSourcePort       INTEGER,
    aclRuleProtocol         INTEGER,
    aclRuleCtrl             INTEGER,
    aclRulePortlist         PortList,
    aclRulePrecedence       INTEGER,
    aclRuleStatus           RowStatus
}

