HARBOUR-ACL-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    IpAddress,
    Counter64           FROM SNMPv2-SMI
    DisplayString,
    RowStatus           FROM SNMPv2-TC
    PortList            FROM Q-BRIDGE-MIB
    harbourAgent        FROM HARBOUR-BASE-MIB;


harbourAcl MODULE-IDENTITY
    LAST-UPDATED    "200311260000Z"
    ORGANIZATION    "Harbour Networks Limited"
    CONTACT-INFO    "www.harbournetworks.com"
    DESCRIPTION
            "Access Control List Management."
    ::= { harbourAgent 4 }


harbourAclServiceStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    up(1),
                    down(2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The status of the acl service is up or down."
    ::= { harbourAcl 1 }


harbourUserMultiDomainGroup     OBJECT IDENTIFIER ::= { harbourAcl 7 }


-- ACL Group

harbourAclGroup                 OBJECT IDENTIFIER ::= { harbourAcl 2 }

-- ACL Group Table

harbourAclGroupTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourAclGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A list of group entries."
    ::= { harbourAclGroup 1 }

harbourAclGroupEntry OBJECT-TYPE
    SYNTAX      HarbourAclGroupEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The access control list group entry."
    INDEX       { harbourAclGroupIndex }
    ::= { harbourAclGroupTable 1 }

HarbourAclGroupEntry ::= SEQUENCE {
    harbourAclGroupIndex        INTEGER,
    harbourAclGroupUsed         INTEGER,
    harbourAclGroupFlag         INTEGER,
    harbourAclGroupName         DisplayString,
    harbourAclGroupStatus       INTEGER
}

harbourAclGroupIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The value of this object uniquely identifies this
            harbourAclGroup entry."
    ::= { harbourAclGroupEntry 1 }

harbourAclGroupUsed OBJECT-TYPE
    SYNTAX      INTEGER {
                    unused(0),
                    used(1)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The access control group entry is or not used."
    ::= { harbourAclGroupEntry 2 }

harbourAclGroupFlag OBJECT-TYPE
    SYNTAX      INTEGER {
                    system(1),
                    config(2),
                    unconfig(3)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The flag of the access control group entry."
    ::= { harbourAclGroupEntry 3 }

harbourAclGroupName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The access control group entry name."
    ::= { harbourAclGroupEntry 4 }

harbourAclGroupStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    valid(1),
                    createRequest(2),
                    underCreation(3),
                    invalid(4)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The access control group entry status."
    ::= { harbourAclGroupEntry 5 }


-- ACL Group Information Table

harbourAclGroupInfoTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourAclGroupInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A list of harbourAclGroupInfo entries."
    ::= { harbourAclGroup 2 }

harbourAclGroupInfoEntry OBJECT-TYPE
    SYNTAX      HarbourAclGroupInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The access control list group infomation entry."
    INDEX       { harbourAclGroupIndex , harbourAclGroupInfoIpAddress }
    ::= { harbourAclGroupInfoTable 1 }

HarbourAclGroupInfoEntry ::= SEQUENCE {
    harbourAclGroupInfoIpAddress    IpAddress,
    harbourAclGroupInfoStatus       INTEGER
}

harbourAclGroupInfoIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The value of this object uniquely identifies this
            harbourAclGroupInfo entry."
    ::= { harbourAclGroupInfoEntry 1 }

harbourAclGroupInfoStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    valid(1),
                    createRequest(2),
                    underCreation(3),
                    invalid(4)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The access control group information entry status."
    ::= { harbourAclGroupInfoEntry 2 }


-- ACL Matrix Group

harbourAclMatrix                OBJECT IDENTIFIER ::= { harbourAcl 3 }

-- ACL Matrix Table

harbourAclMatrixTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourAclMatrixEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A list of matrix entries."
    ::= { harbourAclMatrix 1 }

harbourAclMatrixEntry OBJECT-TYPE
    SYNTAX      HarbourAclMatrixEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The access control list matrix entry."
    INDEX { harbourAclSourceIndex, harbourAclDestinationIndex }
    ::= { harbourAclMatrixTable 1 }

HarbourAclMatrixEntry ::= SEQUENCE {
    harbourAclSourceIndex               INTEGER,
    harbourAclDestinationIndex          INTEGER,
    harbourAclAccess                    INTEGER
}

harbourAclSourceIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The value of this object uniquely identifies this
            harbourAclMatrix entry."
    ::= { harbourAclMatrixEntry 1 }

harbourAclDestinationIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The value of this object uniquely identifies this
            harbourAclMatrix entry."
    ::= { harbourAclMatrixEntry 2 }

harbourAclAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    route(0),
                    deny(1),
                    intervention(2),
                    flowclassify(3),
                    nogroup(4)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The access of the access control list matrix entry."
    ::= { harbourAclMatrixEntry 3 }


-- ACL Source Control Group

harbourAclSourceControl         OBJECT IDENTIFIER ::= { harbourAcl 4 }

