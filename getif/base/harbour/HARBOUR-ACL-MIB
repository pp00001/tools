    SYNTAX      SEQUENCE OF HarbourAclCounterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A list of acl counter entries."
    ::= { harbourAclProtocolFilterGroup 7 }

harbourAclCounterEntry OBJECT-TYPE
    SYNTAX      HarbourAclCounterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The acl counter entry."
    INDEX       { harbourAclCounterIndex }
    ::= { harbourAclCounterTable 1 }

HarbourAclCounterEntry ::= SEQUENCE {
    harbourAclCounterIndex      INTEGER,
    harbourAclCounterName       DisplayString,
    harbourAclCounterData       DisplayString,
    harbourAclCounterStatus     INTEGER,
    harbourAclCounterReset      INTEGER
}

harbourAclCounterIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The index for Acl counter.This value should >1 and <33,
            for we only support 32 counters."
    ::= { harbourAclCounterEntry 1 }

harbourAclCounterName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The name of a counter.The length of this object is <=30(include '\0')."
    ::= { harbourAclCounterEntry 2 }

