-- ACL Custormer Profile Table

harbourCustomerProfileTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourCustomerProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The table of customer profile."
    ::= { harbourUserMultiDomainGroup 1 }

harbourCustomerProfileEntry OBJECT-TYPE
    SYNTAX      HarbourCustomerProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The entry of custom profile."
    INDEX       { customerProfileName }
    ::= { harbourCustomerProfileTable 1 }

HarbourCustomerProfileEntry ::= SEQUENCE {
    customerProfileIndex            INTEGER,
    customerProfileName             DisplayString,
    customerProfileDefaultCmd       INTEGER,
    customerProfilePriority         INTEGER,
    customerProfileDiscardability   INTEGER,
    customerProfileVPT              INTEGER,
    customerProfileTOS              INTEGER,
    customerProfileUpBandWidth      INTEGER,
    customerProfileDownBandWidth    INTEGER,
    customerProfileIP               IpAddress,
    customerProfileMask             IpAddress,
    customerProfileStatus           RowStatus,
    customerProfileBindFlag         INTEGER,
    customerProfileBindToUser       DisplayString
}

customerProfileIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The index of customer profile in the system."
    ::= { harbourCustomerProfileEntry 1 }

customerProfileName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The name of customer profile."
    ::= { harbourCustomerProfileEntry 2 }

customerProfileDefaultCmd OBJECT-TYPE
    SYNTAX      INTEGER {
                    permit(0),
                    deny(1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The default command of customer profile.
            The default setting is permit, the customer can
            access any net resource. If deny is set, the
            customer can only access the net resource appointed
            by access rules."
    ::= { harbourCustomerProfileEntry 3 }

customerProfilePriority OBJECT-TYPE
    SYNTAX      INTEGER (0..3)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The priority of the customer's exchange out queue."
    ::= { harbourCustomerProfileEntry 4 }

customerProfileDiscardability OBJECT-TYPE
    SYNTAX      INTEGER {
                    hold(0),
                    discard(1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This leaf configures whether the system discards
            the packets When the queue congests."
    ::= { harbourCustomerProfileEntry 5 }

customerProfileVPT OBJECT-TYPE
    SYNTAX      INTEGER (0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Configure the parameter VPT in the packets."
    ::= { harbourCustomerProfileEntry 6 }

customerProfileTOS OBJECT-TYPE
    SYNTAX      INTEGER (0..255)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Configure the parameter TOS in the packets."
    ::= { harbourCustomerProfileEntry 7 }

customerProfileUpBandWidth OBJECT-TYPE
    SYNTAX      INTEGER (0..1000)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Restrict the customer's up bandwidth. The granularity
            of the bandwidth is 32kbps."
    ::= { harbourCustomerProfileEntry 8 }

customerProfileDownBandWidth OBJECT-TYPE
    SYNTAX      INTEGER (0..1000)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Restrict the customer's down bandwidth. The granularity
            of the bandwidth is 32kbps."
    ::= { harbourCustomerProfileEntry 9 }

customerProfileIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The IP address appoints to the customer.
            If the entered IP is 0, the system does not restrict the IP address
            configuration for this kind of customer. The system distributes
            an arbitrary IP address for the customer."
    ::= { harbourCustomerProfileEntry 10 }

customerProfileMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The IP net mask appoints to the customer."
    ::= { harbourCustomerProfileEntry 11 }

customerProfileStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The status of a Acl counter."
    ::= { harbourCustomerProfileEntry 12 }

customerProfileBindFlag OBJECT-TYPE
    SYNTAX      INTEGER {
                    isBind(1),
                    notBind(2)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "This leaf indicates whether this profile is
            binding to some ports.
            If you want to delete this profile, you should
            check this flag first."
    ::= { harbourCustomerProfileEntry 13 }

customerProfileBindToUser OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "By this leaf, you can bind or unbind this profile to a user.
            if you want to unbind this profile,please input NONE."
    ::= { harbourCustomerProfileEntry 14 }


-- Customer Profile and ACL Rule Table

harbourCustomerProfileACLRulesTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HarbourCustomerProfileACLRulesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The table of binding ACL rules to customer profile."
    ::= { harbourUserMultiDomainGroup 2 }

harbourCustomerProfileACLRulesEntry OBJECT-TYPE
    SYNTAX      HarbourCustomerProfileACLRulesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "The entry of binding ACL rules to custom profile."
    INDEX       { customerProfileIndex, customerAclRuleIndex }
    ::= { harbourCustomerProfileACLRulesTable 1 }

HarbourCustomerProfileACLRulesEntry ::= SEQUENCE {
    customerAclRuleIndex            INTEGER,
    customerProfileRuleBindStatus   RowStatus
}

customerAclRuleIndex OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The index of ACL rules."
    ::= { harbourCustomerProfileACLRulesEntry 1 }

customerProfileRuleBindStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This leaf binds or disbinds a ACL rule to customer profile."
    ::= { harbourCustomerProfileACLRulesEntry 2 }


