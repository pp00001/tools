    SYNTAX      INTEGER (0..254)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The code of a ICMP layer filter way."
    ::= { harbourAclICMPFilterEntry 7 }

harbourAclICMPFilterCtrl OBJECT-TYPE
    SYNTAX      INTEGER {
                    permit(1),
                    deny(0)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This function is to control the type for a fiter way.
            1 means to permit the destination or source ip,
            0 means to deny."
    ::= { harbourAclICMPFilterEntry 8 }

harbourAclICMPFilterPortlist OBJECT-TYPE
    SYNTAX      PortList
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Especially 000000 means any port."
    ::= { harbourAclICMPFilterEntry 9 }

