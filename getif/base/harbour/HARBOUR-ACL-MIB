    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The Data of a counter."
    ::= { harbourAclCounterEntry 3 }

harbourAclCounterStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    valid(1),
                    createRequest(2),
                    underCreation(3),
                    invalid(4)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The status of a Acl counter."
    ::= { harbourAclCounterEntry 4 }

harbourAclCounterReset OBJECT-TYPE
    SYNTAX      INTEGER {
                    ready(0),
                    reset(1)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "Reset a Acl counter."
    ::= { harbourAclCounterEntry 5 }

harbourAclCounterDeleteAll OBJECT-TYPE
    SYNTAX      INTEGER {
                    ready(0),
                    deleting(1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "To delete all the acl counter."
    ::= { harbourAclProtocolFilterGroup 8 }


-- ACL Rule Table

