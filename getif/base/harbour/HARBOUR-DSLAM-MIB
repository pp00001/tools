    SYNTAX      SEQUENCE OF HbDslamDhcpsPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Dhcp server pool table"
    ::= { hbDslamDhcpServer 4 }

hbDslamDhcpsPoolEntry     OBJECT-TYPE
    SYNTAX      HbDslamDhcpsPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the dhcp pool Table."
    INDEX       { hbDslamDhcpsPoolName  }
    ::= { hbDslamDhcpsPoolTable  1 }

HbDslamDhcpsPoolEntry   ::= SEQUENCE {
    hbDslamDhcpsPoolName                            DisplayString,
    hbDslamDhcpsPoolStatus                          INTEGER,
    hbDslamDhcpsPoolStartIp                         IpAddress,
    hbDslamDhcpsPoolEndIp                           IpAddress,
    hbDslamDhcpsPoolMask                            IpAddress,
    hbDslamDhcpsPoolParamValue                      INTEGER,
    hbDslamDhcpsPoolGateway                         IpAddress,
    hbDslamDhcpsPoolLease                           INTEGER,
    hbDslamDhcpsPoolOptionsDomainName               DisplayString,
    hbDslamDhcpsPoolRowStatus                       RowStatus
}


hbDslamDhcpsPoolName    OBJECT-TYPE
    SYNTAX      DisplayString( SIZE ( 0 .. 8  ) )
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "Dhcp server pool name."
    ::= { hbDslamDhcpsPoolEntry 1 }

hbDslamDhcpsPoolStatus   OBJECT-TYPE
    SYNTAX      INTEGER { inactive(1), active(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Set dhcp server pool status."
    ::= { hbDslamDhcpsPoolEntry  2 }

