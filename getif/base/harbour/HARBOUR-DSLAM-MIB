HARBOUR-DSLAM-MIB    DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    IpAddress           FROM SNMPv2-SMI
    DisplayString,
    MacAddress,
    RowStatus           FROM SNMPv2-TC
    VlanId,
    PortList            FROM Q-BRIDGE-MIB
    DSLAMSeries         FROM HARBOUR-BASE-MIB;


harbourDslam  MODULE-IDENTITY
    LAST-UPDATED    "200403040000Z"
    ORGANIZATION    "Harbour Networks Limited"
    CONTACT-INFO    "www.harbournetworks.com"
    DESCRIPTION     "Dslam character objects."
    ::= { DSLAMSeries 7 }


-- Dslam Vlan Group

hbDslamVlan         OBJECT IDENTIFIER  ::= { harbourDslam 1 }

hbDslamVlanVidMode  OBJECT-TYPE
    SYNTAX      INTEGER { normal(1), l2VidMode(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is used to set the l2 VID mode of the vlan.
            In l2 VID mode, only normal vlan can be used in the system
            and the range of the VID is between 1 to 4095, excluding 2046,
            2047,2944 and 2945.
            If normal VID mode is set, the range of VID is 1-2045 and
            3200-4095."
    ::= { hbDslamVlan 1 }

hbDslamVlanL2PortSeperateTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF HbDslamVlanL2PortSeperateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Vlan L2 port seperate table"
    ::= { hbDslamVlan 2 }

hbDslamVlanL2PortSeperateEntry OBJECT-TYPE
    SYNTAX      HbDslamVlanL2PortSeperateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbour vlan l2 port seperate Table."
    INDEX       { vlanL2PortSeperateID  }
    ::= { hbDslamVlanL2PortSeperateTable 1 }

HbDslamVlanL2PortSeperateEntry ::= SEQUENCE {
    vlanL2PortSeperateID                       VlanId,
    vlanL2PortSeperateStatus                   INTEGER
}

vlanL2PortSeperateID OBJECT-TYPE
    SYNTAX      VlanId
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The vlan Id used as index."
    ::= { hbDslamVlanL2PortSeperateEntry 1 }

vlanL2PortSeperateStatus OBJECT-TYPE
    SYNTAX      INTEGER { disable(1), enable(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is to enable or disable the l2 port seperation
            function of the vlan."
    ::= { hbDslamVlanL2PortSeperateEntry 2 }

-- Dslam Vlan Broadcast Restrain Group

hbDslamVlanBroadcastRestrainGroup  OBJECT IDENTIFIER  ::= { hbDslamVlan 3}

hbDslamVlanBroadcastRestrain OBJECT-TYPE
    SYNTAX      INTEGER { disable(1), enable(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is used to set the broadcasting restraining function."
    ::= { hbDslamVlanBroadcastRestrainGroup 1 }

hbDslamVlanBroadcastRestrainTimeWindow OBJECT-TYPE
    SYNTAX      INTEGER (1..16)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is used to set the broadcasting restraining
            time window."
    ::= { hbDslamVlanBroadcastRestrainGroup 2 }

hbDslamVlanBroadcastRestrainCounter OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is used to set the broadcasting restraining counter."
    ::= { hbDslamVlanBroadcastRestrainGroup 3 }

hbDslamVlanBroadcastRestrainCounterUnit OBJECT-TYPE
    SYNTAX      INTEGER { packets(1), bytes(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This object is used to set the broadcasting restraining
            counter's unit."
    ::= { hbDslamVlanBroadcastRestrainGroup 4 }

-- Dslam Vlan Batch Group

hbDslamVlanBatchGroup    OBJECT IDENTIFIER  ::= { hbDslamVlan  4 }

hbDslamVlanBatchOperateStartVID  OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Before batch creating or deleting Vlans of the system,
            you should set the start VID of these vlans."
    ::= { hbDslamVlanBatchGroup  1 }

hbDslamVlanBatchOperateEndVID  OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Before batch creating or deleting Vlans of the system,
            you should make clear how many vlans you want to
            create or delete."
    ::= { hbDslamVlanBatchGroup 2 }

hbDslamVlanBatchOperateUserPort OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Before batch creating Vlans of the system,
            you should make clear from which port you want to create vlan."
    ::= { hbDslamVlanBatchGroup 3 }

hbDslamVlanBatchOperateUplinkPort OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Before batch creating Vlans of the system,
            you should make clear which port you want to
            join to the creating vlan with a tag as the uplink port."
    ::= { hbDslamVlanBatchGroup  4 }

hbDslamVlanBatchOperateControl OBJECT-TYPE
    SYNTAX      INTEGER {
                    createVlan(1),
                    deleteVlan(2),
                    addUserPorts(3),
                    addUplinkPorts(4),
                    deleteUserPorts(5),
                    deleteUplinkPorts(6),
                    noOperate(7)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "To create or delete vlans by a group.
            If the vlans are already created as a group,
            you can add or delete ports for these vlans.
            After creating or deleting, please notice
            the harbourVlanBatchOperateErrorMsg."
    ::= { hbDslamVlanBatchGroup 5 }


-- Dslam PPPoE Group

hbDslamPPPoE           OBJECT IDENTIFIER  ::= { harbourDslam 2 }
l2PPPoEForwordGroup    OBJECT IDENTIFIER  ::= { hbDslamPPPoE 1 }

l2PPPoEForwordConfigMode      OBJECT-TYPE
    SYNTAX      INTEGER {
                    best(1),
                    better(2),
                    normal(3),
                    none(4),
                    ext1(5),
                    ext2(6),
                    ext3(7),
                    ext4(8)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The config mode of the system."
    ::= { l2PPPoEForwordGroup 1 }

l2PPPoEForwordUserBindCtrl OBJECT-TYPE
    SYNTAX      INTEGER { disable(1), enable(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This leaf is used in mode better and normal.
            Enable or disable the level 2 PPPoE forwording userbind
            function."
    ::= { l2PPPoEForwordGroup 2}

l2PPPoEForwordBRASMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This leaf is used in mode better and normal.
            The MAC address of the l2 PPPoE forwording."
    ::= { l2PPPoEForwordGroup 3}

-- L2PPPoE Forward Config Vlan Table

l2PPPoEForwardConfigVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF L2PPPoEForwardConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "layer2 PPPoE Forward Config table"
    ::= { l2PPPoEForwordGroup 4 }

l2PPPoEForwardConfigVlanEntry OBJECT-TYPE
    SYNTAX      L2PPPoEForwardConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An entry in the harbour Forward VlanConfigTable."
    INDEX       { l2PPPoEForwardvlanID  }
    ::= { l2PPPoEForwardConfigVlanTable 1 }

L2PPPoEForwardConfigVlanEntry ::= SEQUENCE {
    l2PPPoEForwardvlanID                       INTEGER,
    l2PPPoEFilterNonePPPoEPacketsStatus        INTEGER,
    l2PPPoEForwordVlanUserBindCtrlStatus       INTEGER
}

l2PPPoEForwardvlanID OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The vlan Id used as index."
    ::= { l2PPPoEForwardConfigVlanEntry 1 }


l2PPPoEFilterNonePPPoEPacketsStatus OBJECT-TYPE
    SYNTAX      INTEGER { disable(1), enable(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This leaf is only used in best mode.
            Appointed a vlan to filter the none PPPoE packets."
    ::= { l2PPPoEForwardConfigVlanEntry 2}

l2PPPoEForwordVlanUserBindCtrlStatus OBJECT-TYPE
    SYNTAX      INTEGER { disable(1), enable(2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This leaf is only used in best mode, please config the vlan
            name before enable the l2 PPPoE forwording fuction."
    ::= { l2PPPoEForwardConfigVlanEntry 3}


