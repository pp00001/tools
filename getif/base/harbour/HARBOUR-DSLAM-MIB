    ::= { hbDslamDhcpsPoolEntry 7 }

hbDslamDhcpsPoolLease      OBJECT-TYPE
    SYNTAX      INTEGER (1..525600)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Set dhcp server pool lease. vlaue is 1..525600.
            If set 0xffffffff, means infinite."
