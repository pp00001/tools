    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Set dhcp server pool ip mask ."
    ::= { hbDslamDhcpsPoolEntry 5 }

hbDslamDhcpsPoolParamValue   OBJECT-TYPE
    SYNTAX      INTEGER (1..565200)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Set dhcp server pool param value. vlaue is 1..565200.
            If set 0xffffffff, means infinite."
    ::= { hbDslamDhcpsPoolEntry 6 }

hbDslamDhcpsPoolGateway     OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
