NETSCREEN-SET-DHCP-MIB DEFINITIONS ::= BEGIN
IMPORTS
    IpAddress FROM RFC1155-SMI

    DisplayString 	FROM RFC1213-MIB 
    netscreenSetting FROM NETSCREEN-SMI;
    
    nsSetDHCP OBJECT IDENTIFIER ::= { netscreenSetting 6 }

	nsSetDHCPService OBJECT-TYPE
        SYNTAX  INTEGER {
        		none(0),
        		dhcp_relay_agent(1),
        		dhcp_server(2)
        }
    	ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "DHCP service type for trusted network."
        ::= { nsSetDHCP 1 }
        
	nsSetDHCPRelayServer OBJECT-TYPE
        SYNTAX  DisplayString(SIZE(0..32))
    	ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "DHCP relay agent server name."
        ::= { nsSetDHCP 2 }
        
	nsSetDHCPVpnEncryp OBJECT-TYPE
        SYNTAX  INTEGER {
        		disable(0),
        		enabled(1)
        }
    	ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "secure DHCP relay agent traffic via VPN encrption."
        ::= { nsSetDHCP 3 }

END
